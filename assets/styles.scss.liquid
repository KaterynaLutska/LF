@mixin responsive-font-size($min-font-size, $max-font-size, $min-screen-width) {
  font-size: $max-font-size;

  @media all and (max-width: 992px) {
    font-size: calc(#{$font-size-header-collection} - 1vw) !important;
   }
 @media all and (max-width: 767px) {
    font-size: calc(#{$font-size-header-collection} - 1.5vw) !important;
   }
 @media all and (max-width: 420px) {
    font-size: calc(#{$font-size-header-collection} - 2.5vw) !important;
   }



  @if $max-font-size < $min-font-size {
    @media all and (max-width: $min-screen-width) {
      font-size: calc(#{$font-size-header-collection} - 1vw) !important;
    }
  }

  @if $max-font-size > $min-font--mobile-size {
    @media all and (max-width: 420px) {
      font-size: calc(#{$font-size-header-collection} - 5vw) !important;

    }
  }
}

