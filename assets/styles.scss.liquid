  @media (min-width: 992px) {
    &.-animate--zoom-in:hover {
      .gallery-item-img {
        transform: scale(1.2);
      }
    }
    &.-animate--zoom-out {
      .gallery-item-img {
        transform: scale(1.2);
      }
      &:hover {
        .gallery-item-img {
          transform: scale(1);
        }
      }
    }

    &.-animate--overlay-in,
    &.-animate--overlay-out {
      .gallery-item-overlay {
        display: block;
      }
    }

    &.-animate--fade-in {
      .gallery-item-img {
        opacity: .64;
      }
      &:hover .gallery-item-img {
        opacity: 1;
      }
    }

    &.-animate--fade-out {
      .gallery-item-img {
        opacity: 1;
      }
      &:hover .gallery-item-img {
        opacity: .64;
      }
    }

    &.-animate--grayscale-in {
      .gallery-item-img {
        filter: grayscale(100%);
      }
      &:hover .gallery-item-img {
        filter: grayscale(0);
      }
    }

    &.-animate--grayscale-out {
      .gallery-item-img {
        filter: grayscale(0);
      }
      &:hover .gallery-item-img {
        filter: grayscale(100%);
      }
    }

    &.-animate--blur-in,
    &.-animate--blur-out {
      .gallery-item-img {
        transform: scale(1.1);
      }
    }

    &.-animate--blur-in:hover {
      .gallery-item-img {
        filter: blur(10px);
      }
    }
    &.-animate--blur-out {
      .gallery-item-img {
        filter: blur(10px);
      }
